		"body": [
			"${1:column}: Sequelize.FLOAT$2,"       		
		],
		"description": "sequelizeColumFLOAT$2"
	},

	"sequelizeColumINTERGER": {
		"prefix": "ccmsi",
		"body": [
			"${1:column}: Sequelize.INTERGER$2,"       
				
		],
